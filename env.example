# API ключи для языковых моделей
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1
# Используется модель: gpt-4.1-2025-04-14 (новая версия)
# Fallback модели: gpt-4.1, gpt-4

# DeepSeek API (прямое подключение)
# Формат модели: deepseek-reasoner или deepseek-chat
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_BASE=https://api.deepseek.com/v1

# Claude API через ProxyAPI (альтернатива DeepSeek)
# Формат модели: claude-sonnet-4-20250514 или claude-sonnet-4
ANTHROPIC_API_KEY=your_proxyapi_key_here
ANTHROPIC_API_BASE=https://api.proxyapi.ru/anthropic

